<?xml version="1.0"?>
<robot name="material_mesh_texture">
  <link name="link_box">
    <visual>
      <geometry>
        <box size="0.5 0.5 0.5"/>
      </geometry>
    </visual>
  </link>
  <link name="link_obj">
    <visual>
      <geometry>
        <mesh filename="assets/box_with_texture.obj" scale="0.5 0.5 0.5"/>
      </geometry>
    </visual>
  </link>

  <joint name="joint_box_obj" type="fixed">
    <origin rpy="0 0 0" xyz="1 0 0"/>
    <parent link="link_box"/>
    <child link="link_obj"/>
  </joint>
</robot>
